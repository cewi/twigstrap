{
    "name": "Cewi/twigstrap",
    "description": "Use Twig Templates for Bootstrap 4 in CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "twig",
        "boostrap4"
    ],
    "homepage": "https://github.com/cewi/twigstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Cornelius Wichmann",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/cewi/twigstrap/issues",
        "source": "https://github.com/cewi/twigstrap"
    },
    "require": {
        "php": ">=5.4",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "~3.4",
        "wyrihaximus/twig-view": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cewi\\Twigstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cewi\\Twigstrap\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
